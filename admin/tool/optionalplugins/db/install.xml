<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="admin/tool/optionalplugins/db" VERSION="20220119" COMMENT="XMLDB file for Moodle admin/tool/optionalplugins"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="tool_optionalplugins_log" COMMENT="Log table for optional plugin installations">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="installed" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="alreadyinstalled" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="notinstalled" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
