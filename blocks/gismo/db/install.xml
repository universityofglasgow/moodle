        <FIELD NAME="userid" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="course" NEXT="restype"/>
        <FIELD NAME="restype" TYPE="char" LENGTH="50" NOTNULL="true" SEQUENCE="false" PREVIOUS="userid" NEXT="resid"/>
        <FIELD NAME="resid" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="restype" NEXT="timedate"/>
        <FIELD NAME="timedate" TYPE="text" LENGTH="small" NOTNULL="true" SEQUENCE="false" PREVIOUS="resid" NEXT="time"/>
        <FIELD NAME="time" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="timedate" NEXT="numval"/>
        <FIELD NAME="numval" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="time"/>
