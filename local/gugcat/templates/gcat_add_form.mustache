{{!
    @template local_gugcat/form


    Example context (json):
    {
    
    }
}}

<div class="form-container">
    <h5 class="title">{{addnewgrade}}</h5>
    <div class="mform-container">
        <div class="form-group row">
            <div class="col-md-3">
            <label>Course</label>
            </div>
            <div class="col-md-9 form-inline">
            <span>{{course.fullname}}</span></div>
        </div> 
        <div class="form-group row">
            <div class="col-md-3">
            <label>Assessment</label>
            </div>
            <div class="col-md-9 form-inline">{{section}}</div>
        </div>
        <div class="form-group row">
            <div class="col-md-3">
            <label>Surname</label>
            </div>
            <div class="col-md-9 form-inline felement"> {{student.lastname}}</div>
        </div> 
        <div class="form-group row">
            <div class="col-md-3">
            <label>Forname</label>
            </div>
            <div class="col-md-9 form-inline felement"> {{student.firstname}}</div>
        </div> 
        <div class="form-group row">
            <div class="col-md-3">
            <label>1st Grade</label>
            </div>
        <div class="col-md-9 form-inline felement">{{gbgrade}}</div>
    </div>
</div>
