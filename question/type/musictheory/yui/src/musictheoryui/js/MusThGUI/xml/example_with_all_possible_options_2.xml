<?xml version="1.0" encoding="UTF-8"?>

<MusThGUI
  xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
  xsi:noNamespaceSchemaLocation='valid_input_xml.xsd' canvasEditable="true" accidentalCarryOver="true">
  <StaffSystem maxLedgerLines="3">
    <Staff clef="treble">
      <KeySign totalAccColumns="7" >
        <Accidental type="#" letter="F" register="5" editable="false" />
        <Accidental type="#" letter="C" register="5" editable="false" />
        <Accidental type="#" letter="G" register="5" editable="false" />
      </KeySign>
      <NoteColumns>
        <NoteColumn type="two-voice" maxNotes="2">
          <Note letter="A" register="4" accidental="n" noteValue="quarter_stem_down" editable="false" />
        </NoteColumn>
        <NoteColumn maxNotes="1" />
        <NoteColumn maxNotes="1" />
        <NoteColumn maxNotes="1" />
        <NoteColumn maxNotes="1" />
        <NoteColumn maxNotes="1" />
        <NoteColumn maxNotes="1" />
        <NoteColumn maxNotes="1" />
      </NoteColumns>
    </Staff>
    <Staff clef="bass">
      <KeySign totalAccColumns="7" >
        <Accidental type="#" letter="F" register="3" editable="false" />
        <Accidental type="#" letter="C" register="3" editable="false" />
        <Accidental type="#" letter="G" register="3" editable="false" />
      </KeySign>
      <NoteColumns>
        <NoteColumn type="two-voice" maxNotes="2">
          <FiguredBass>
            <Row text="/6" />
            <Row text="#4" />
            <Row text="n3" />
          </FiguredBass>
          <TextInput value="" editable="true" />
        </NoteColumn>
        <NoteColumn type="two-voice" maxNotes="2">
          <TextInput value="" editable="true" />
        </NoteColumn>
        <NoteColumn type="two-voice" maxNotes="2">
          <TextInput value="" editable="true" />
        </NoteColumn>
        <NoteColumn type="two-voice" maxNotes="2">
        </NoteColumn>
        <NoteColumn type="two-voice" maxNotes="2">
          <TextInput value="" editable="true" />
        </NoteColumn>
        <NoteColumn type="two-voice" maxNotes="2">
          <TextInput value="" editable="true" />
        </NoteColumn>
        <NoteColumn type="two-voice" maxNotes="2">
          <TextInput value="" editable="true" />
        </NoteColumn>
        <NoteColumn type="two-voice" maxNotes="2"></NoteColumn>
      </NoteColumns>
    </Staff>
  </StaffSystem>
  <Toolbars>
    <AccidentalToolbar>
      <Button symbol="n" />
      <Button symbol="#" />
      <Button symbol="b" />
      <Button symbol="x" />
      <Button symbol="bb" />
    </AccidentalToolbar>
    <NoteValueToolbar>
      <Button symbol="quarter_stem_up" />
      <Button symbol="quarter_stem_down" />
    </NoteValueToolbar>
  </Toolbars>
  <InputDialog id="MusicInputDialogVoiceLeading" OKButtonText="OK" cancelButtonText="Cancel" width="300" height="130" headerContent="Input for Harmonic Functions">
    <DialogHTML><![CDATA[
      <div id="dialog-form-voice-leading">
        <div style="padding:15px; ">
          <form>
            <fieldset style="border:none">
              <select>
                <option value=""></option>
                <option value="I">I</option>
                <option value="ii">ii</option>
                <option value="iii">iii</option>
                <option value="IV">IV</option>
                <option value="V">V</option>
                <option value="vi">vi</option>
                <option value="viio">viio</option>
              </select>
              <select>
                <option value=""></option>
                <option value="6">6</option>
                <option value="64">64</option>
                <option value="65">65</option>
                <option value="43">43</option>
                <option value="42">42</option>
              </select>
              <select>
                <option value=""></option>
                <option value="/ii">/ii</option>
                <option value="/iii">/iii</option>
                <option value="/IV">/IV</option>
                <option value="/V">/V</option>
                <option value="/vi">/vi</option>
              </select>
            </fieldset>
          </form>
        </div>
      </div>
      ]]></DialogHTML>
  </InputDialog>
</MusThGUI>