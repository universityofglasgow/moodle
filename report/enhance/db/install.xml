<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="report/enhance/db" VERSION="20180209" COMMENT="XMLDB file for Moodle report/enhance"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="report_enhance" COMMENT="Default comment for report_enhance, please edit me">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="status" TYPE="char" LENGTH="30" NOTNULL="true" SEQUENCE="false" COMMENT="Status of request ('Approved', 'Approved in development', 'Approved released', 'Pending', 'Proposed', 'Rejected'"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="description" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="benefits" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="department" TYPE="char" LENGTH="50" NOTNULL="false" SEQUENCE="false" COMMENT="College or school"/>
        <FIELD NAME="reason" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="strategy" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="benefit" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="impact" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="consultations" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="remit" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="sustainability" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="security" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="robustness" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="support" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="availability" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="functionality" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="uniqueness" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="timescales" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="resources" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="status" UNIQUE="false" FIELDS="status"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>
